{
    "image": "mcr.microsoft.com/devcontainers/javascript-node:latest",
    "overrideCommand": true,
    "runArgs": [
        "--cap-add=SYS_PTRACE",
        "--security-opt",
        "seccomp=unconfined"
    ],
    "customizations": {
        "vscode": {
            "extensions": [
                "streetsidesoftware.code-spell-checker",
                "dbaeumer.vscode-eslint",
                 "DavidKaiser.kaiser-project-zip"
            ]
        }
    }

}
